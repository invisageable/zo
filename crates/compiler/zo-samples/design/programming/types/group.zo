-- types: group.

type Foo = int
  and Bar = (float, int)
  and Oof = float[]
  and Rab = Fn(int): int;

fun main() {
  imu w: Foo = 0;
  
  showln("{w}");

  imu x: Bar = (1.0, 200);

  showln("{x}");

  imu y: Oof = [1, 2, 3, 4];

  showln("{y}");

  imu z: Rab = fn(x) -> x * x;

  showln("{}", z(2));
}
